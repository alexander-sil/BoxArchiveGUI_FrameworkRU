<Window x:Name="MainForm" x:Class="BoxArchiveGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoxArchiveGUI"
        mc:Ignorable="d"
        Title="Архиватор" Height="150" Width="160">
    <Grid x:Name="MainGrid" Margin="0,0,0,-3">
        <Button x:Name="CompressButton" Content="Упаковать" HorizontalAlignment="Center" Margin="0,42,0,0" VerticalAlignment="Top" Width="80" Height="25" Click="CompressButton_Click"/>
        <Button x:Name="DecompressButton" Content="Распаковать" HorizontalAlignment="Center" Margin="0,81,0,0" VerticalAlignment="Top" Width="80" Height="25" Click="DecompressButton_Click"/>
    </Grid>
</Window>
